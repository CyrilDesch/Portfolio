{
  "name": "portfolio2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@parcel/transformer-glsl": "^2.6.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "react": "^17.0.2",
    "react-burger-menu": "^3.0.6",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-scroll": "^1.8.2",
    "react-social-icons": "^5.5.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "parcel serve --port 3000 --dist-dir build",
    "build:development": "parcel build --target developmentBuild",
    "build:production": "parcel build --target productionBuild"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "source": "public/index.html",
  "targets": {
    "developmentBuild": {
      "distDir": "build",
      "scopeHoist": false
    },
    "productionBuild": {
      "distDir": "build",
      "sourceMap": false,
      "scopeHoist": false
    }
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "2.4.1",
    "@parcel/reporter-bundle-analyzer": "^2.4.1",
    "@parcel/reporter-bundle-buddy": "^2.4.1",
    "@parcel/transformer-less": "2.4.1",
    "@parcel/transformer-webmanifest": "2.4.1",
    "@parcel/validator-eslint": "^2.4.1",
    "parcel": "^2.4.1"
  }
}
